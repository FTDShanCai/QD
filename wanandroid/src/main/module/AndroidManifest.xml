<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.wanandroid">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".WanApp"
        android:allowBackup="true"
        android:icon="@mipmap/wan_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/wan_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.usuallyNet.WanUsuallyNetActivity"></activity>
        <activity android:name=".ui.articleList.WanArticleListActivity" />
        <activity android:name=".ui.systemData.WanSystemDataActivity" />
        <activity android:name=".ui.main.WanMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>